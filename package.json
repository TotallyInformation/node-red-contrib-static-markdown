{
  "name": "node-red-contrib-static-markdown",
  "version": "0.1.1",
  "description": "Serve up a folder of static markdown files from Node-RED",
  "scripts": {
    "push": "git push origin",
    "pull": "git pull origin",
    "presync": "npm run pull",
    "sync": "npm run push",
    "listbin": "ls ./node_modules/bin",
    "npmtags": "npm dist-tag ls node-red-contrib-staticMarkdown",
    "npmtagnext": "npm dist-tag add node-red-contrib-staticMarkdown@$npm_package_version next",
    "gittags": "git tag",
    "gittag": "git tag -a v$npm_package_version",
    "gitpushtags": "git push origin --tags"
  },
  "dependencies": {
    "@gerhobbelt/markdown-it-footnote": "^3.0.2-4",
    "@gerhobbelt/markdown-it-include": "^1.1.3-7",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "js-yaml": "^3.14.0",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-front-matter": "^0.2.1",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-kbd": "^2.2.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-mermaid-fence-new": "0.0.3",
    "markdown-it-playground": "0.0.5",
    "markdown-it-prism": "^2.1.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "sanitize-html": "^1.27.4"
  },
  "devDependencies": {
    "@types/jquery": "*",
    "@types/node-red": "^1.1.0",
    "replace-in-file": "^6.1.0"
  },
  "node-red": {
    "version": ">=0.19.0",
    "nodes": {
      "staticMarkdown": "nodes/staticMarkdown.js"
    },
    "notyetnodes": {}
  },
  "browserslist": [
    "> 0.5%",
    "maintained node versions",
    "last 2 versions",
    "not dead",
    "not ie < 11"
  ],
  "directories": {
    "bin": "./bin"
  },
  "bin": {},
  "main": "nodes/staticMarkdown.js",
  "engines": {
    "node": ">=10.0"
  },
  "types": "docs/mytypes.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TotallyInformation/node-red-contrib-staticMarkdown.git"
  },
  "keywords": [
    "node-red",
    "markdown",
    "static-serve",
    "markdown-it"
  ],
  "author": {
    "name": "Julian Knight",
    "url": "https://github.com/TotallyInformation"
  },
  "contributors": [
    {
      "name": "Julian Knight",
      "url": "https://github.com/TotallyInformation"
    },
    {
      "name": "Janvda",
      "url": "https://github.com/janvda"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TotallyInformation/node-red-contrib-staticMarkdown/issues"
  },
  "homepage": "https://github.com/TotallyInformation/node-red-contrib-staticMarkdown#readme"
}
